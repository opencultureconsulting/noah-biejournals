                    <titleInfo lang="eng">
                        <title>What does Political Participation Mean to Spanish Students?</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Sant, Edda</displayForm>
                        <namePart type="family">Sant</namePart>
                        <namePart type="given">Edda</namePart>
                        <role>
